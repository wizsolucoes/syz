{
  "name": "ng-syz",
  "version": "0.10.6",
  "scripts": {
    "ng": "ng",
    "app": "ng serve app",
    "build": "npm run build:lib:prod && npm run build:app:prod",
    "build:app": "ng build app",
    "build:app:prod": "ng build app --configuration production",
    "build:lib": "ng build ng-syz",
    "build:lib:prod": "ng build ng-syz --configuration production",
    "build:lib:watch": "ng build ng-syz --watch",
    "test": "npm run test:lib && npm run test:app",
    "test:coverage": "npm run test:lib:coverage && npm run test:app:coverage",
    "test:app": "ng test app --watch=false --browsers=\"ChromeHeadless\"",
    "test:app:coverage": "ng test app --watch=false --browsers=\"ChromeHeadless\" --code-coverage",
    "test:lib": "ng test ng-syz --watch=false --browsers=\"ChromeHeadless\"",
    "test:lib:coverage": "ng test ng-syz --watch=false --browsers=\"ChromeHeadless\" --code-coverage",
    "test:app:watch": "ng test app --browsers=\"ChromeHeadless\"",
    "test:lib:watch": "ng test ng-syz --browsers=\"ChromeHeadless\"",
    "lint": "npm run lint:lib && npm run lint:app",
    "lint:app": "ng lint app",
    "lint:lib": "ng lint ng-syz",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "markdown-it": "^12.1.0",
    "ngx-mask": "^12.0.0",
    "ngx-swiper-wrapper": "^10.0.0",
    "rxjs": "~6.6.0",
    "swiper": "^6.8.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.1",
    "@angular/cli": "^13.2.1",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.20",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.5.5"
  }
}