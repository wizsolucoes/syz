{
  "name": "ng-syz",
  "version": "0.7.3",
  "scripts": {
    "ng": "ng",
    "app": "ng serve app",
    "build": "npm run build:lib:prod && npm run build:app:prod",
    "build:app": "ng build app",
    "build:app:prod": "ng build app --prod",
    "build:lib": "ng build ng-syz",
    "build:lib:prod": "ng build ng-syz --prod",
    "build:lib:watch": "ng build ng-syz --watch",
    "test": "npm run test:lib && npm run test:app",
    "test:coverage": "npm run test:lib:coverage && npm run test:app:coverage",
    "test:app": "ng test app --watch=false --browsers=\"ChromeHeadless\"",
    "test:app:coverage": "ng test app --watch=false --browsers=\"ChromeHeadless\" --code-coverage",
    "test:lib": "ng test ng-syz --watch=false --browsers=\"ChromeHeadless\"",
    "test:lib:coverage": "ng test ng-syz --watch=false --browsers=\"ChromeHeadless\" --code-coverage",
    "test:app:watch": "ng test app --browsers=\"ChromeHeadless\"",
    "test:lib:watch": "ng test ng-syz --browsers=\"ChromeHeadless\"",
    "lint": "npm run lint:lib && npm run lint:app",
    "lint:app": "ng lint app",
    "lint:lib": "ng lint ng-syz",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.6",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "~11.0.6",
    "@angular/compiler": "~11.0.6",
    "@angular/core": "~11.0.6",
    "@angular/forms": "~11.0.6",
    "@angular/material": "^11.2.10",
    "@angular/platform-browser": "~11.0.6",
    "@angular/platform-browser-dynamic": "~11.0.6",
    "@angular/router": "~11.0.6",
    "markdown-it": "^12.1.0",
    "ngx-mask": "^11.1.5",
    "ngx-swiper-wrapper": "^10.0.0",
    "rxjs": "~6.6.0",
    "swiper": "^6.7.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "^11.2.12",
    "@angular/compiler-cli": "~11.0.6",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.2"
  }
}
